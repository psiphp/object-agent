            "Psi\\Component\\ObjectAgent\\Tests\\": "tests/",
            "Psi\\Bridge\\ObjectAgent\\Doctrine\\PhpcrOdm\\Tests\\": "bridge/doctrine-phpcr-odm/tests/",
            "Psi\\Bridge\\ObjectAgent\\Doctrine\\Orm\\Tests\\": "bridge/doctrine-orm/tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
